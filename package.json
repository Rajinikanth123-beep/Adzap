{
  "name": "adzap-app",
  "version": "0.1.0",
  "private": true,
 
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.13.5",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "mongoose": "^8.19.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently -k -n BACKEND,FRONTEND \"npm:backend:dev\" \"npm:start:frontend\"",
    "start:frontend": "set HOST=0.0.0.0&& react-scripts start",
    "start:local": "react-scripts start",
    "backend:start": "node backend/server.js",
    "backend:dev": "nodemon backend/server.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "gh-pages": "^6.3.0",
    "nodemon": "^3.1.10"
  }
}
